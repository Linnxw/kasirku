{
  "compilerOptions": {
    "target": "es2016",                 
    "module": "commonjs",
     "baseUrl": "./",
     "paths": {
       "*":["node_modules/*"]
      },
    "typeRoots": ["./src/types","./node_modules/@types"],          
    "sourceMap": true,                  
    "outDir": "./bin",                   
    "esModuleInterop": true,             
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}